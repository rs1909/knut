vfname()=mackey_glass;
time()=t;
H0=1;
CC=15;
cfa=(270.0/161.0);
cfb=(-72.0/161.0);
cfc=(5.0/69.0);
period(T)=0.4;
par(kk1)=0;
par(zeta)=0.015;
par(tau)=1.5*2*Pi;
DX1=delay(X1,tau);
DDX1=delay(X1,2*tau);
AD=H0+DDX1-DX1;
A1=H0+DX1-X1;
A2=2*H0+DDX1-X1;
cg2AD=(1.0+tanh(CC*AD))/2.0;
cg2mAD=(1.0+tanh(-CC*AD))/2.0;
cfA1=A1*( cfa + cfb*A1 + cfc*A1*A1 );
cfA2=A2*( cfa + cfb*A2 + cfc*A2*A2 );
scf=cfA1*cg2AD + cfA2*cg2mAD;
dot(X1)=X2;
init(X1)=0;
dot(X2)=-X1 - 2*zeta*X2 + kk1*scf;
init(X2)=0;
