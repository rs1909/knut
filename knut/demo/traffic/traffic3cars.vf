vfname()=traffic3cars;
time()=t;
period(T)=1.1;
par(LpN)=2;
par(tau)=1;
par(alpha)=1;
par(vv0)=1.0;
H2=3*LpN-H0-H1;
D(H0)=delay(H0,tau);
V(H0)=heaviside(D(H0)-1)*((D(H0) - 1)^3.0) / ((D(H0) - 1)^3.0 + 1);
dot(V0)=alpha * ( vv0 * V(H0) - V0 );
init(V0)=0.5;
dot(V1)=alpha * ( vv0 * V(H1) - V1 );
init(V1)=0.5;
dot(V2)=alpha * ( vv0 * V(H2) - V2 );
init(V2)=0.5;
dot(H0)=V1-V0;
init(H0)=2;
dot(H1)=V2-V1;
init(H1)=2;
